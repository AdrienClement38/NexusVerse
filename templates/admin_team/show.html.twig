{% extends 'base.html.twig' %}

{% block title %}Team{% endblock %}
{% block pageTitle %}Team{% endblock %}

{% block bodyContent %}
    <h1>Team</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ team.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ team.name }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ team.country }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_team_index') }}">back to list</a>

    <a href="{{ path('app_admin_team_edit', {'id': team.id}) }}">edit</a>

    {{ include('admin_team/_delete_form.html.twig') }}
{% endblock %}
